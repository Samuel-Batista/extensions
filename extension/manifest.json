{
    "manifest_version": 2,
    "name": "Documentos Google off-line",
    "version": "3.14",
    "browser_action": {
		"default_icon": "download.png",
		"default_title": "Documentos Google off-line"
	},

    "icons": {
        "16": "download.png",
        "32": "download.png",
        "48": "download.png",
        "128": "download.png"
      },

    "description": "Edite, crie e veja seus documentos, planilhas e apresentações sem precisar de acesso à Internet.",

    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "css" : ["main.css"],
            "js" : ["main.js"]
        }
    ]
}